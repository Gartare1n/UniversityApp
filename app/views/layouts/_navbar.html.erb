<nav>
  <div class="container">
    <div class="nav-wrapper">
      <a href="#!" class="brand-logo"><i class="material-icons">cloud</i>University</a>
      <ul class="right hide-on-med-and-down">
        <% if logged_in? %>
          <li><a href="<%= student_path(current_user) %>"><i class="material-icons">account_circle</i></a></li>
        <% end %>
        <li><a href="<%= root_path %>"><i class="material-icons">event_note</i></a></li>
        <% if logged_in?%>
          <li><a href="<%= students_path %>"><i class="material-icons">contacts</i></a></li>
        <% end %>
        <li><%= session_link%></li>
      </ul>
    </div>
  </div>
</nav>
<% flash.each do |key, message| %>
  <div class="container">
    <div class="row">
      <div class="col s12 m12">
        <div class="card-panel green lighten-2" id="fade-out-target">
          <span class="white-text">
            <%= message %>
          </span>
        </div>
      </div>
    </div>
  </div>
<% end %>